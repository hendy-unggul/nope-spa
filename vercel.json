{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "sw.js",
      "use": "@vercel/static",
      "headers": {
        "Content-Type": "application/javascript"
      }
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0",
        "Pragma": "no-cache",
        "Expires": "0",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/(.*).html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    },
    {
      "src": "/manifest.json",
      "headers": {
        "Cache-Control": "no-cache, max-age=0",
        "Content-Type": "application/manifest+json"
      }
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/register.html",
      "dest": "/register.html"
    },
    {
      "src": "/dunia.html",
      "dest": "/dunia.html"
    },
    {
      "src": "/jalan.html",
      "dest": "/jalan.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
